    <conf name="Arduino_mega2560" type="3">
      <toolsSet>
        <compilerSet>Arduino-1.8.10|GNU</compilerSet>
        <dependencyChecking>true</dependencyChecking>
        <rebuildPropChanged>true</rebuildPropChanged>
      </toolsSet>
      <compileType>
        <cTool>
          <developmentMode>0</developmentMode>
          <incDir>
            <pElem>${ARDUINO_PATH}/hardware/arduino/avr/cores/arduino</pElem>
            <pElem>${ARDUINO_PATH}/hardware/arduino/avr/variants/crumbuino128</pElem>
          </incDir>
          <commandLine>-Os -Wextra -std=gnu11 -ffunction-sections -fdata-sections -MMD -mmcu=atmega128a -flto</commandLine>
          <preprocessorList>
            <Elem>ARDUINO=10802</Elem>
            <Elem>ARDUINO_ARCH_AVR</Elem>
            <Elem>F_CPU=16000000L</Elem>
          </preprocessorList>
        </cTool>
        <ccTool>
          <developmentMode>0</developmentMode>
          <incDir>
            <pElem>${ARDUINO_PATH}/hardware/arduino/avr/cores/arduino</pElem>
            <pElem>${ARDUINO_PATH}/hardware/arduino/avr/variants/mega</pElem>
            <pElem>UTFT</pElem>
            <pElem>../libarduinoext/include</pElem>
          </incDir>
          <commandLine>-Os -Wextra -std=gnu++11 -fpermissive -fno-exceptions -ffunction-sections -fdata-sections -fno-threadsafe-statics -MMD -mmcu=atmega2560 -fno-rtti -flto -mcall-prologues</commandLine>
          <preprocessorList>
            <Elem>ARDUINO=10810</Elem>
            <Elem>ARDUINO_ARCH_AVR</Elem>
            <Elem>ARDUINO_AVR_MEGA2560</Elem>
            <Elem>F_CPU=16000000L</Elem>
            <Elem>__AVR__</Elem>
          </preprocessorList>
        </ccTool>
        <fortranCompilerTool>
          <developmentMode>5</developmentMode>
        </fortranCompilerTool>
        <asmTool>
          <developmentMode>5</developmentMode>
        </asmTool>
        <archiverTool>
          <commandlineTool>avr-gcc-ar</commandlineTool>
          <ranlibTool>avr-gcc-ranlib</ranlibTool>
        </archiverTool>
      </compileType>
